{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach RT Local Dev",
      "type": "node",
      "port": 9229,
      "request": "attach"
    },
    {
      "name": "Launch RT Local Dev",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
      "args": ["${workspaceRoot}/src/index.ts"],
      "restart": true,
      "outputCapture": "std",
      "env": {
        "LOG_LEVEL": "debug"
      },
      "skipFiles": ["<node_internals>/**", "node_modules/**/*.js"]
    }
  ]
}
